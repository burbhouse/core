{
  "name": "burbhouse-core",
  "version": "1.0.0",
  "author": "David Cabrera <david@rerainc.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "css-loader": "^3.0.0",
    "gatsby-image": "^2.0.38",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.1.0",
    "gatsby-plugin-sharp": "^2.0.34",
    "gatsby-plugin-styled-components": "^3.1.0",
    "gatsby-source-contentful": "^2.0.48",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.1.18",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-burger-menu": "^2.6.10",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "sharp": "^0.22.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "contentful-import": "^7.4.3",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby": "^2.3.22",
    "gh-pages": "^2.0.1",
    "inquirer": "^6.2.2",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "yargs-parser": "^13.0.0"
  },
  "main": "n/a",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/burbhouse/core.git"
  },
  "scripts": {
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "standard --fix",
    "build": "gatsby build",
    "add-domain": "echo \"core.burbhouse.com\" > public/CNAME",
    "deploy": "gatsby build --prefix-paths && yarn run add-domain && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "postinstall": "node ./bin/hello.js",
    "setup": "node ./bin/setup.js",
    "start": "npm run develop",
    "heroku-postbuild": "gatsby build",
    "cleanup-repository": "rimraf bin contentful && yarn remove contentful-import chalk inquirer rimraf "
  }
}
